package {
    default_applicable_licenses: ["hardware_generic_audio_license"],
}

apex {
    name: "com.android.hardware.audio.generic",
    manifest: "manifest.json",
    file_contexts: "file_contexts",
    key: "com.android.hardware.key",
    certificate: ":com.android.hardware.certificate",
    updatable: false,
    vendor: true,

    binaries: [
        "android.hardware.audio.service-aidl.generic",
        "android.hardware.audio.effect.service-aidl.generic",
    ],
    native_shared_libs: [
        "libaecsw_generic",
        "libagc1sw_generic",
        "libagc2sw_generic",
        "libbassboostsw_generic",
        "libdownmixsw_generic",
        "libdynamicsprocessingsw_generic",
        "libenvreverbsw_generic",
        "libequalizersw_generic",
        "liberasersw_generic",
        "libextensioneffect_generic",
        "libhapticgeneratorsw_generic",
        "libloudnessenhancersw_generic",
        "libnssw_generic",
        "libpresetreverbsw_generic",
        "libspatializersw_generic",
        "libvirtualizersw_generic",
        "libvisualizersw_generic",
        "libvolumesw_generic",
        "libeffectconfig_generic",
    ],
    prebuilts: [
        "android.hardware.audio.service-aidl.generic.rc",
        "android.hardware.audio.service-aidl.generic.xml",
    ],
}
